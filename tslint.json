{
  "extends": [
    "tslint:latest"
  ],
  "rules": {
    "quotemark": [
      true, "single"
    ],
    "eofline": true,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
     "ordered-imports": [
      false
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "no-object-literal-type-assertion": true,
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "space-before-function-paren": [
      true,
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always",
        "method": "never",
        "constructor": "never"
      }
    ],
    "object-literal-sort-keys": false,
    "no-string-literal": false,
    "no-submodule-imports": false,
    "no-namespace": [
      true,
      "allow-declarations"
    ]
  },
  "jsRules": {
    "quotemark": [
      true, "single"
    ],
    "object-literal-sort-keys": false
  }
}